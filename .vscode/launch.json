    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "(gdb) Launch image.load",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/src/engine/image/test/image.load.filesystem/image.load.filesystem",
            "args": [
                "${workspaceFolder}/src/engine/image/test/image.load.filesystem/test.png"
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb"
        },
